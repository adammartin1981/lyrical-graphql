{
  "name": "lyrical",
  "version": "1.0.0",
  "description": "Starter point for a graphQL course",
  "main": "index.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/StephenGrider/Lyrical-GraphQL"
  },
  "scripts": {
    "dev": "nodemon",
    "types": "apollo codegen:generate --target=typescript --localSchemaFile=schema.json --includes=**/*.tsx output",
    "schema": "apollo schema:download --endpoint http://localhost:4000/graphql schema.json",
    "updateTypes": "yarn dev && yarn schema && yarn types"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/mongoose": "^5.5.1",
    "apollo": "^2.11.1",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link-http": "^1.5.14",
    "axios": "^0.15.3",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.16.0",
    "connect-mongo": "^1.3.2",
    "css-loader": "^0.26.1",
    "express": "^4.14.0",
    "express-graphql": "^0.8.0",
    "express-session": "^1.15.0",
    "graphql": "14.3.0",
    "graphql-tag": "^2.10.1",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.4",
    "mongodb": "^3.2.4",
    "mongoose": "^4.7.8",
    "nodemon": "*",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "react": "15.4.2",
    "react-apollo": "^2.5.5",
    "react-dom": "15.4.2",
    "react-router": "^3.0.2",
    "style-loader": "^0.13.1",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5",
    "webpack": "^2.2.0",
    "webpack-dev-middleware": "^1.9.0"
  }
